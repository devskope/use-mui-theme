{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "10.0.0"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "env": {
    "test": {
      "plugins": ["istanbul"]
    },
    "cjs": {
      "presets": [["@babel/preset-env", { "modules": "commonjs" }]]
    },
    "esm": {
      "presets": [["@babel/preset-env", { "modules": false, "loose": true }]]
    }
  }
}
