{
  "name": "@devskope/use-mui-theme",
  "version": "1.0.0",
  "description": "Returns a custom material-ui theme and a callback to modify the theme value",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib",
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "babel --delete-dir-on-start --copy-files --env-name esm --out-file-extension .mjs src --out-dir lib",
    "build:cjs": "babel  --env-name cjs src --out-dir lib",
    "coverage": "nyc --reporter=lcov --reporter=text mocha",
    "test": "nyc mocha"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "devskope",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devskope/useMuiTheme.git"
  },
  "bugs": {
    "url": "https://github.com/devskope/useMuiTheme/issues"
  },
  "homepage": "https://github.com/devskope/useMuiTheme#readme",
  "license": "MIT",
  "keywords": [
    "hook",
    "hooks",
    "material-ui",
    "material ui",
    "mui",
    "react",
    "theme"
  ],
  "peerDependencies": {
    "@material-ui/core": "^4.4.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@material-ui/core": "^4.4.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jsdom": "^16.2.2",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1",
    "prop-types": "^15.7.2"
  },
  "dependencies": {}
}
